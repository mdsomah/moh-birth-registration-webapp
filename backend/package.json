{
  "name": "backend",
  "version": "1.0.0",
  "description": "<!-- Ministry of Health (MOH) Online Birth Registration System Backend -->",
  "type": "commonjs",
  "main": "./src/server.js",
  "scripts": {
    "start": "node ./src/server.js",
    "build": "webpack --config webpack.config.js",
    "dev": "nodemon ./src/server.js",
    "prod": "nodemon ./src/server.js",
    "start-dev": "cross-env NODE_ENV=development npm run dev",
    "start-prod": "cross-env NODE_ENV=production npm run prod",
    "backend-frontend": "concurrently \"npm run start-dev\" \"cd ../frontend && npm start\" ",
    "migrate-dev": "npx prisma migrate dev",
    "migrate-prod": "npx prisma migrate deploy",
    "reset-dev": "npx prisma migrate reset",
    "debug": "cross-env DEBUG=ROMMTech-eSales-POS-Backend npm run start-dev",
    "lint:fix": "npx eslint --fix ./*.js",
    "test": "mocha"
  },
  "keywords": [
    "Backend",
    "APIs",
    "Database"
  ],
  "author": "Mesheal D. Somah",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "babel-loader": "^10.0.0",
    "bcrypt": "^6.0.0",
    "colors": "^1.4.0",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cookie-session": "^2.1.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "debug": "^4.4.1",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.5.0",
    "express-slow-down": "^2.1.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "keygen": "^0.4.0",
    "keygrip": "^1.1.0",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "multer": "^2.0.1",
    "nanoid": "^5.1.5",
    "nodemailer": "^7.0.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "passport-remember-me": "^0.0.1",
    "statuses": "^2.0.2",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "yup": "^1.6.1",
    "yup-phone-lite": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.29.0",
    "html-loader": "^5.1.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.9.0",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  }
}
